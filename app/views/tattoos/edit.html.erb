<% form_for @tattoo do |f| %>
  <%= f.error_messages %>

  <fieldset>
    <%= f.label :title, "title" %><br />
    <%= f.text_field :title %>
  </fieldset>
  <fieldset>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </fieldset>
  
    
  <fieldset>
    <%= f.submit "Save" %>
  </fieldset>
  
<% end %>
